<Configuration status="all">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%highlight{%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n}"/>
        </Console>
    </Appenders>
    <Loggers>
        <logger name="org.unideb" >
            <level value="all"/>
        </logger>
    </Loggers>
</Configuration>